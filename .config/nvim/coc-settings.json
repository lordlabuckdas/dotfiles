{
	"suggest.maxCompleteItemCount": 10,
	"coc.preferences.formatOnSaveFiletypes": [
		"python",
		"json",
		"rust"
	],
	"python.venvPath": "$HOME/.virtualenvs",
	"python.formatting.provider": "black",
	"python.formatting.blackPath": "~/.virtualenvs/neovim3/bin/black",
	"python.formatting.blackArgs": [
		"-l",
		"120"
	],
	"python.linting.flake8Enabled": true,
	"python.linting.flake8Path": "~/.virtualenvs/neovim3/bin/flake8",
	"python.linting.flake8Args": [
		"--max-line-length",
		"120"
	],
	"rust-analyzer.server.path": "~/.local/bin/rust-analyzer"
}
